MRF1
{
    cellZone    movingRR;

    // Fixed patches (by default they 'move' with the MRF zone)
    nonRotatingPatches (bodywork, tarmac, lateralArea, symmetryPlane);

    origin      (-1.726285 -1.00115  -0.428600);
    axis        (0 1 0);

    rpm         67.887126;
}

