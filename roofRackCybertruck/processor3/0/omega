FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 200;

boundaryField
{
    tarmac
    {
        type            omegaWallFunction;
        value           uniform 200;
    }
    upperArea
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 200;
    }
    outlet
    {
        type            zeroGradient;
    }
    lateralArea
    {
        type            zeroGradient;
    }
    symmetryPlane
    {
        type            symmetry;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 200;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 200;
    }
}


