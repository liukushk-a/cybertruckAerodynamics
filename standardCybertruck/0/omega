FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 440.15;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    
    "(outlet|upperArea|lateralArea)"
    {
        type            zeroGradient;
    }

    "(tarmac|cybertruckRealMeasuring)"
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    symmetryPlane
    {
        type            symmetry;
    }
}


// ************************************************************************* //
